{
  "cli": {
    "version": ">=3.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "pre-install": "bash eas-build-pre-install.sh",
      "ios": {
        "buildConfiguration": "Debug",
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "pre-install": "bash eas-build-pre-install.sh",
      "ios": {
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "pre-install": "bash eas-build-pre-install.sh",
      "ios": {
        "buildConfiguration": "Release",
        "enterpriseProvisioning": "adhoc",
        "scheme": "appconsegnetest",
        "image": "latest",
        "cache": {
          "disabled": true
        }
      },
      "android": {
        "buildType": "app-bundle",
        "image": "latest",
        "cache": {
          "disabled": true
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}