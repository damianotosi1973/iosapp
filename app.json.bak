      "package": "com.damianotosi.consegneaziendale",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "4ae6d32a-7607-4ca3-b616-7e3af770f67e"
      }
